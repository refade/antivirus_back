<script language="JavaScript">
function calcHeight()
{    
    var dodai_trang = document.getElementById('Shacker').contentWindow.document.body.scrollHeight;
	//alert(dodai_trang);
	if(dodai_trang > 500)
    	document.getElementById('Shacker').height=dodai_trang;
	else
		 document.getElementById('Shacker').height=500;
}
function change_background() {
var iframeDoc;
if (window.frames && window.frames.Shacker && (iframeDoc = window.frames.Shacker.document)) {
// now use iframeDoc here
iframeDoc.body.style.backgroundColor='#000000';
//alert(iframeDoc.body.style.backgroundColor);
}

}
</script>


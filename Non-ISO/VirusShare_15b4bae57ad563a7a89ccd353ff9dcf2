<script type="text/javascript">

/***********************************************
* ProHTML Ticker script- © Dynamic Drive (www.dynamicdrive.com)
* This notice must stay intact for use
* Visit http://www.dynamicdrive.com/ for full source code
***********************************************/

var tickspeeda=4000 //ticker speed in miliseconds (2000=2 seconds)
var enablesubjecta=1 //enable scroller subject? Set to 0 to hide

if (document.getElementById){
document.write('<style type="text/css">\n')
document.write('.dropcontent{display:none;}\n')
document.write('</style>\n')
}

var selectedDiva=0
var totalDivsa=0

function contractalla(){
var inc=0
while (document.getElementById("dropmsga"+inc)){
document.getElementById("dropmsga"+inc).style.display="none"
inc++
}
}


function expandonea(){
var selectedDivObja=document.getElementById("dropmsga"+selectedDiva)
contractalla()
/*document.getElementById("dropcontentsubjecta").innerHTML=selectedDivObja.getAttribute("subject")*/
selectedDivObja.style.display="block"
selectedDiva=(selectedDiva<totalDivsa-1)? selectedDiva+1 : 0
setTimeout("expandonea()",tickspeeda)
}

function startscrollera()
{
while (document.getElementById("dropmsga"+totalDivsa)!=null)
totalDivsa++
expandonea()
if (!enablesubjecta)
document.getElementById("dropcontentsubjecta").style.display="none"

}

if (window.addEventListener)
window.addEventListener("load", startscrollera, false)
else if (window.attachEvent)
window.attachEvent("onload", startscrollera)

</script>


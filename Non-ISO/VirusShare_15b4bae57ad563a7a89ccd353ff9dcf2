<script language="javascript">

/*****************************/



function checkVote(f,num,lang) {

	var len = f.vote.length;

	var checked = false;

	w = 500;

	h = 300;

	var left = (screen.availWidth/2) - (w/2);

	var top = (screen.availHeight/2) - (h/2);



	if(num == "1") {

		window.open('binhchon/votes.php?lang='+lang,'vote','width='+w+',height='+h+',scrollbars=0,status=0, left='+left+', top='+top)

		return true;

		

	} else {

		for (var i = 0; i< len; i++) {

			if (f.vote[i].checked==true){

				window.open('binhchon/votes.php?vote=' + f.vote[i].value + '&lang='+lang,'vote','width='+w+',height='+h+',left='+left+',top='+top);

				return true;

			}

		}

	}

	alert("Xin cho biet su lua chon cua ban!");

	f.vote.focus;

	return false;

}

</script>  

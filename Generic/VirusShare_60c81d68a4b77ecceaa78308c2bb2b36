    var lineColor = "#ffffff";
    var width = 260;
    var isVertical = true;
    // t : stage_height;
    // l : stage_width
    // k : isVertical
    // e : pitch_height
    // f : pitch_width
    // a : lineColor

    var stage_width = width, 
    stage_height = isVertical ? 3 * stage_width / 2 : 2 * stage_width / 3,
    pitch_width = stage_width - 20, pitch_height = stage_height - 20;

    var stage = new Kinetic.Stage({ container: "canvas-container", width: stage_width, height: stage_height });
    pitch_layer = new Kinetic.Layer;
    home_layer = new Kinetic.Layer;
    away_layer = new Kinetic.Layer;
    title_layer = new Kinetic.Layer;

    var playerTextRect = new Kinetic.Rect({width: 160,height: 50, cornerRadius: 8,fill: "#000",opacity: 0.7,visible: 1});
    var playerText = new Kinetic.Text({text: "asdasd", width: 130,fontFamily: "sans-serif",fontSize: 11,padding: 5,visible: 1,fill: "#fff"});
    var A = new Kinetic.RegularPolygon({x: 100,y: 100,rotationDeg: 120,sides: 3,radius: 6,opacity: 0.7,visible: 1,fill: "#000"});
    var b = new Kinetic.Rect({ x: 10, y: 10, width: pitch_width, height: pitch_height, stroke: lineColor, strokeWidth: 2 }),
    d = new Kinetic.Rect({ x: isVertical ? 10 + 0.375 * pitch_width : 10, y: isVertical ? 10 : 10 + 0.375 * pitch_height, width: isVertical ? pitch_width / 4 : pitch_width / 20, height: isVertical ? pitch_height / 20 : pitch_height / 4, stroke: lineColor, strokeWidth: 2 }),
    n = new Kinetic.Rect({ x: isVertical ? 10 + 0.25 * pitch_width : 10, y: isVertical ? 10 : 10 + 0.25 * pitch_height, width: isVertical ? pitch_width / 2 : 3 * pitch_width / 20, height: isVertical ? 3 * pitch_height / 20 : pitch_height / 2, stroke: lineColor, strokeWidth: 2 }),
    q = new Kinetic.Rect({ x: isVertical ? 10 + 0.375 * pitch_width : pitch_width + 10 - pitch_width / 20, y: isVertical ? pitch_height + 10 - pitch_height / 20 : 10 + 0.375 * pitch_height, width: isVertical ? pitch_width / 4 : pitch_width / 20, height: isVertical ? pitch_height / 20 : pitch_height / 4, stroke: lineColor, strokeWidth: 2 }),
    w = new Kinetic.Rect({ x: isVertical ? 10 + 0.25 * pitch_width : pitch_width + 10 - 3 * pitch_width / 20, y: isVertical ? 10 + pitch_height - 3 * pitch_height / 20 : 10 + 0.25 * pitch_height, width: isVertical ? pitch_width / 2 : 3 * pitch_width / 20, height: isVertical ? 3 * pitch_height / 20 : pitch_height / 2, stroke: lineColor, strokeWidth: 2 }),
    h = new Kinetic.Line({ points: isVertical ? [10, 10 + pitch_height / 2, 10 + pitch_width, 10 + pitch_height / 2] : [10 + pitch_width / 2, 10, 10 + pitch_width / 2, 10 + pitch_height], stroke: lineColor, strokeWidth: 2 }),

    p = new Kinetic.Circle({ x: stage_width / 2, y: stage_height / 2, radius: 4, fill: lineColor }),
    r = new Kinetic.Circle({ x: isVertical ? stage_width / 2 : 10 + 0.9 * pitch_width, y: isVertical ? 10 + 0.1 * pitch_height : stage_height / 2, radius: 4, fill: lineColor });

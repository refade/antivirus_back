    <script>
        $(document).ready(function(){
            $(".match-info-wrapper .mac-plus-link").click();
        });
        function teamImgLoadErr(obj) {
            obj.src = "http://im.cdn.md/img5/team-no-logo.gif";
        }
        function getLiveStanding() {
            var url = APP_ROOT + '/AjaxHandlers/StandingHandler.aspx?command=matchStanding&id=2264359&sv=1';
            $.ajax({
                url: url,
                success: function (data) {
                    $("#dvStanding").html(data);
                },
                error: function () { window.setTimeout(getLiveStanding, 30000); }
            });
        }

        
        function getRBStats() {
            var url = APP_ROOT + '/AjaxHandlers/MatchHandler.aspx?command=rbStats&id=2264359';
            $.ajax({
                url: url,
                success: function(data) {
                    $("#dvRBStats").html(data);
                },
                error: function() { window.setTimeout(getRBStats, 60000); }
            });
        }
        
        function getOtherMatches() {
            var url = APP_ROOT + '/AjaxHandlers/StandingHandler.aspx?command=otherMatches&id=2264359';
            $.ajax({
                url: url,
                success: function(data) {
                    $("#dvOtherMatches").html(data);
                },
                error: function() { window.setTimeout(getOtherMatches, 60000); }
            });
        }
        
        function getOPTATeamStats() {
            var url = APP_ROOT + '/AjaxHandlers/MatchHandler.aspx?command=optaStats&id=2264359';
            $.ajax({
                url: url,
                success: function(data) {
                    $("#dvOPTAStats").html(data);
                },
                error: function() { window.setTimeout(getOPTATeamStats, 60000); }
            });
        }
        

        function sticky_relocate() {
            var window_top = $(window).scrollTop();
            var div_top = $('#sticky-anchor').offset().top;
            if (window_top > div_top) {
                $('#sticky').addClass('stick');
                $('#sticky-anchor').height($('#sticky').outerHeight());
            } else {
                $('#sticky').removeClass('stick');
                $('#sticky-anchor').height(0);
            }
        }

        $(function () {
            $(window).scroll(sticky_relocate);
            sticky_relocate();
        });
    </script>

        //$(document).ready(function () {
        //    if ((typeof (advertisement_not_blocked) === 'undefined' || advertisement_not_blocked !== 1) && window.location.href.indexOf("adblocker.aspx") == -1) {
        //        window.location.href = "http://www.sahadan.com/adblocker.aspx";

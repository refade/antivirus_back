<script>
    function shortenName (strName) {
        var result = strName;
        var nameArr = [];
        if (strName.indexOf(' ') > 0) {
            nameArr = strName.split(' ');
        }
        if (nameArr.length > 1) {
            //result = nameArr[0].charAt(0) + "." + nameArr[nameArr.length-1];
            result = nameArr[nameArr.length-1];
        }
        //console.log(strName + '-' + result);
        return result;        
    }

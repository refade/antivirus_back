    <script>
        AdmostClient.getBanner(87);

        //$(document).ready(function () {
        //    if ((typeof (advertisement_not_blocked) === 'undefined' || advertisement_not_blocked !== 1) && window.location.href.indexOf("adblocker.aspx") == -1) {
        //        window.location.href = "http://www.sahadan.com/adblocker.aspx";
        //    }
        //});


        var cont = 0;
        function sticky_relocate_master() {

            var window_top = $(window).scrollTop();
            var div_top = $('#sticky-anchor-master').offset().top;
            if (window_top > div_top && cont == 0) {
                $('#sticky-master').addClass('stick-master');
                $('#sticky-anchor-master').height($('#sticky-master').outerHeight());
                setTimeout(function () {
                    $('#sticky-master').removeClass('stick-master');
                    $('#sticky-anchor-master').height(0);
                    //alert("23");
                    cont = 1;
                }, 2000);

            } else {
                $('#sticky-master').removeClass('stick-master');
                $('#sticky-anchor-master').height(0);
            }
        }

        $(function () {
            $(window).scroll(sticky_relocate_master);
            sticky_relocate_master();
        });

    </script>
    
    

    ca = new Kinetic.Circle({ x: 10 + pitch_width, y: 10 + pitch_height, radius: pitch_width / 60, startAngle: 1, endAngle: 1.5, stroke: lineColor, strokeWidth: 2 }),
    u = new Kinetic.Circle({ x: isVertical ? stage_width / 2 : 10 + pitch_width / 10, y: isVertical ? 10 + pitch_height / 10 : stage_height / 2, radius: isVertical ? pitch_height / 12 : pitch_width / 12, startAngle: isVertical ? 0.2 : 1.7, endAngle: isVertical ? 0.8 : 0.3, stroke: lineColor, strokeWidth: 2 }),
    a = new Kinetic.Circle({ x: isVertical ? stage_width / 2 : 10 + pitch_width - pitch_width / 10, y: isVertical ? 10 + 0.9 * pitch_height : stage_height / 2, radius: isVertical ? pitch_height / 12 : pitch_width / 12, startAngle: isVertical ? 1.2 : 0.7, endAngle: isVertical ? 1.8 : 1.3, stroke: lineColor, strokeWidth: 2 });
    */
    pitch_layer.add(b);
    pitch_layer.add(d);
    pitch_layer.add(q);
    pitch_layer.add(n);
    pitch_layer.add(w);
    pitch_layer.add(h);
    pitch_layer.add(v);
    pitch_layer.add(p);
    pitch_layer.add(r);
    pitch_layer.add(m);

    pitch_layer.on("click", 
        function() {
            clear_title()
        });
    pitch_layer.on("tap", function() {
            clear_title()
        })
    /*
    c.add(s);
    c.add(aa);
    c.add(ba);
    c.add(ca);
    c.add(u);
    c.add(a);
    */
    stage.add(pitch_layer);

    var player_circles = [], nameArr = [];
    var player_shirtNos = [];
    // t : stage_height;
    // l : stage_width
    // k : isVertical
    // e : pitch_height
    // f : pitch_width
    // a : lineColor

    draw_player = function (index, player) {
        // c : index
        // m : player_x
        // u : player_y
        // h : player

        if (player.x == 0 || player.y == 0) {
            return;
        }
        if (isVertical) {
            player.y = 100 - player.y;
        }
        var player_x = isVertical ? 10 + player.y / 100 * pitch_width : 10 + player.x / 200 * pitch_width,
        player_y = isVertical ? 10 + player.x / 200 * pitch_height : 10 + player.y / 100 * pitch_height;
        
        if (player.side == "away") {
            isVertical ? player_y = stage_height - player_y : player_x = stage_width - player_x;
        }
        var layer = player.side == "home" ? home_layer : away_layer;

        player_circles[index] = new Kinetic.Circle({ x: player_x, y: player_y, radius: 9, stroke: "#000", fill: player.side == "home" ? "#3069b0" : "#ea7814", strokeWidth: 1 });
        var shirtNo = new Kinetic.Text({ x: player_x - 15, y: player_y - 6, width: 30, height: 30, text: player.shirt, fontSize: 10, fontFamily: "Verdana", fontStyle:"bold", fill: "#fff", align:"center", verticalAlign: "middle" });
        nameArr[index] = new Kinetic.Text({ x: player_x - 40, y: player_y + 10, width: 80, height: 15, text: shortenName(player.name), fontSize: 10, fontStyle:"bold", fontFamily: "Verdana", fill: "#000", align: "center", verticalAlign: "middle",shadowColor: 'white', shadowBlur: 2, shadowOffset: 1, shadowOpacity: 1 });
        
        player_circles[index].on("click", function () {
            showPlayerDetail(player, player_x, player_y);
        });

        player_circles[index].on("tap", function() {
            showPlayerDetail(player, player_x, player_y);
        });
        player_circles[index].on("mouseover", function() {
            document.body.style.cursor = "pointer"
        });
        player_circles[index].on("mouseout", function() {
            document.body.style.cursor = "default"
        });
        layer.add(player_circles[index]);
        layer.add(shirtNo);
        layer.add(nameArr[index]);

        
    };
    clear_title = function() {
        title_layer.removeChildren();
        title_layer.draw();
    };
    draw_crest = function (side, image) {
        var crest_width = 70;
        var crest_height = 70, 
        crest_x, crest_y;
        isVertical ? (crest_x = (stage_width - crest_width) / 2, crest_y = "home" === side ? 10 + 0.25 * pitch_height - crest_height / 2 : 10 + 0.75 * pitch_height - crest_height / 2) : 
                     (crest_y = (stage_height - crest_height) / 2, crest_x = "home" === side ? 10 + 0.25 * pitch_width - crest_height / 2 : 10 + 0.75 * pitch_width - crest_height / 2);
        /*
        if ("home" === side || "away" === side)
            isVertical ? crest_y = pitch_height / 2 - crest_height / 2 + 10 : crest_x = stage_height / 2 + 5;
            */
        c = new Kinetic.Image({x: crest_x, y: crest_y, opacity: 0.40,image: image,visible: !0,width: crest_width,height: crest_height});            
        if(side=="home") {
            home_layer.add(c);
        } else {
            away_layer.add(c);
        }
    }
    
    homeCrest = new Image;
    homeCrest.src = "http://im.cdn.md/img/logo/buyuk/1.gif";
    homeCrest.onload = function() {
        draw_crest("home", homeCrest);
        for (i = 0; i < homePlayerList.length; i++) {
            draw_player(i, homePlayerList[i]);
        }
        stage.add(home_layer);
    }
   
    awayCrest = new Image;
    awayCrest.src = "http://im.cdn.md/img/logo/buyuk/452.gif";
    awayCrest.onload = function() {
        draw_crest("away", awayCrest);
        for (i = 0; i < awayPlayerList.length; i++) {
            draw_player(i, awayPlayerList[i]);
        } 
        stage.add(away_layer);
    }
    

    function showPlayerDetail(player, x, y) {
        debugger;
        var title_x = x;
        var title_y = y;
        var v = y + 25;
        title_layer.add(playerTextRect)
        title_layer.add(playerText);
        title_layer.add(A);
        
        0 > title_x - 80 ? title_x = 80 : title_x + 80 > stage_width && (title_x += stage_width - (title_x + 80));
        80 > stage_height - title_y ? (title_y -= 106, v -= 50, A.setRotationDeg(180)) : A.setRotationDeg(120);
        
        playerTextRect.setPosition(title_x - 80, title_y + 28);
        playerTextRect.show();
        playerText.setText(player.name + "\nPass Yüzdesi: %" + player.pass + "\nŞut: " + player.sot + "/" + player.ts);
        playerText.setPosition(title_x - 76, title_y + 30)
        playerText.show();
        A.setPosition(x, v);

        title_layer.draw();
        stage.add(title_layer);
        /*
        
        A.show();
        W(a, k);
        title_layer.draw();
        J = g.playerNames[b.id];
        K = b.id;
        L = b.teamid;
        M = a;
        N = g.teams["home" === a ? 0 : 1].name;
        $jqOpta.events.trigger.click(d, "player_popup", {player_name: J,player_id: K,team_id: L,side: M,team_name: N,action: "show"})
        */
    }
    
</script>
                        </div>
                        


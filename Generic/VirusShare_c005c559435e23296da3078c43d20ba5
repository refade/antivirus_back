<script src="https://cdn.radiantmediatechs.com/rmp/4.5.9/js/rmp.min.js" integrity="sha384-oGWyIkTHLtRotRmiJZKGgbq+ndrzW4s153hVxOCnldm7jAulE7C98ZLX0RKxqbAF" crossorigin="anonymous"></script>
<script>
    (function () {
        'use strict';

        $(".rmpPlayer").hide(); //hide player div by default

        var _adTagUrl = 'https://pubads.g.doubleclick.net/gampad/ads?sz=640x480&iu=/86855500/outstream&cust_params=domain%3d%26section%3dpatrol%26tag_list%3dduty-deaths%2cohio%2cpolice-vehicle-theft%2cdanville-oh-pdBCSEGMENTS&impl=s&gdfp_req=1&env=vp&output=vast&unviewed_position_start=1&url=[referrer_url]&description_url=[description_url]&correlator=[timestamp]';
        _adTagUrl = _adTagUrl.replace('BCSEGMENTS', encodeURIComponent('&bc_segment=' + _bcSegmentStr));

        // player settings
        var settings = {
            licenseKey: 'ZXlqdmJmY2lvbUAxNTA5NDc2',
            delayToFade: 3000,
            // these are maximum values
            //width: 640,
            //height: 480,
            ads: true,
            // we enabled outstream ads
            adOutStream: true,
            // we tell the player we want the in-article UI
            adOutStreamInArticle: true,
            // we tell the player we want to muted autoplay the ad with hover API enabled
            adOutStreamInArticleMutedAutoplayWithHoverApi: true,
            // when the outstream ad is over a reload icon should appear
            // when interacted with this icon should cause the outstream player to reload the provided adTag
            adTagReloadOnEnded: true,
            // when the ad is clicked do not pause the ad
            adPauseOnClick: false,
            // apply a nice background color to fit your article background
            backgroundColor: 'DDDDDD',
            // our adTagUrl
            adTagUrl: _adTagUrl,
            // our waterfall adTagUrls if required
            adTagWaterfall: []
        };

        // Reference to the player containers (id)
        var elementID = 'rmpPlayer_%ecid!';

        // New Radiant Media Player instance
        var rmp = new RadiantMP(elementID);

        // player container
        var rmpContainer = document.getElementById(elementID);

        // show on player when ad is ready
        addEvent(rmpContainer, 'adloaded', function () {
            $(".rmpPlayer").slideDown("fast");
        })

        // destroy player on completion of video
        addEvent(rmpContainer, 'adalladscompleted', function () {
            $(".rmpPlayer").slideUp("fast");
            rmp.destroy();
            removeEvent(document, 'scroll');
        })

        // destroy player if autoplay failure
        addEvent(rmpContainer, 'autoplayfailure', function () {
            rmp.destroy();
            removeEvent(document, 'scroll');
        })

        // remove player from view
        addEvent(rmpContainer, 'destroycompleted', function () {
            var parentContainer = rmpContainer.parentNode;
            if (parentContainer) {
                parentContainer.removeChild(rmpContainer);
            }
        })

        // Set the name of the hidden property and the change event for visibility
        var hidden, visibilityChange;
        if (typeof document.hidden !== 'undefined') { // Opera 12.10 and Firefox 18 and later support 
            hidden = 'hidden';
            visibilityChange = 'visibilitychange';
        } else if (typeof document.msHidden !== 'undefined') {
            hidden = 'msHidden';
            visibilityChange = 'msvisibilitychange';
        } else if (typeof document.webkitHidden !== 'undefined') {
            hidden = 'webkitHidden';
            visibilityChange = 'webkitvisibilitychange';
        }

        // Warn if the browser doesn't support addEventListener or the Page Visibility API
        if (typeof document.addEventListener === 'undefined' || typeof document[hidden] === 'undefined') {
            console.log('Page Visibility API not supported');
        } else {
            // Handle page visibility change   
            document.addEventListener(visibilityChange, handleVisibilityChange);
        }

        // pause/play when video is in view while scrolling
        var scrollEventHandler = function () {
            if (isScrolledIntoView(rmpContainer)) {
                rmp.play();
            } else {
                rmp.pause();
            }
        }

        addEvent(document, 'scroll', scrollEventHandler)

        // Initialize player
        rmp.init(settings);

        // utility functions

        // If the page is hidden, pause the video. If the page is shown, play the video
        function handleVisibilityChange() {
            if (document[hidden]) {
                rmp.pause();
            } else {
                rmp.play();
            }
        }

        function isScrolledIntoView(el) {
            var elemTop = el.getBoundingClientRect().top;
            var elemBottom = el.getBoundingClientRect().bottom;

            var isVisible = (elemTop >= 0) && (elemBottom <= window.innerHeight);
            return isVisible;
        }

        // for older browsers (IE8) that don't support addEventListener event binding natively
        function addEvent(el, type, handler) {
            if (el.attachEvent) el.attachEvent('on' + type, handler); else el.addEventListener(type, handler);
        }
        function removeEvent(el, type, handler) {
            if (el.detachEvent) el.detachEvent('on' + type, handler); else el.removeEventListener(type, handler);
        }
    })(); 
</script>





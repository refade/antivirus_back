<script language="javascript">
  function mouseIn(LongState, ShortState) {
     document.getElementById('Notes').innerHTML = LongState;
	 document.getElementById('AU-Map').src = "images/AU-states-" + ShortState + ".gif"
	 document.getElementById(ShortState + '-link').className = "hover";
  }

  function mouseOut(ShortState) {
     document.getElementById('Notes').innerHTML = '';
	 document.getElementById('AU-Map').src = "images/AU-states-main.gif"
	 document.getElementById(ShortState + '-link').className = "body";	 
  }
</script>


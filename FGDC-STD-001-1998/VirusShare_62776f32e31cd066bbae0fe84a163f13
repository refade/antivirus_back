<script type='text/javascript' src='http://www.alnada.com/nada/wp-includes/js/wp-embed.min.js?ver=4.4.2'></script>


<script src="http://www.alnada.com/nada/wp-content/themes/hereps/js/jquery.cookie.js"></script>
<script type="text/javascript">
	// jQuery Cookie Plugin v1.4.0
	// https://github.com/carhartl/jquery-cookie
	jQuery.noConflict();
	
	var cookie_expire_new = 3; //set cookie var
	var cookie_expire_old = jQuery.cookie('newsbt_expire');
	if (cookie_expire_new != cookie_expire_old) { //if equals this val
		jQuery(window).load(function() {
			jQuery.removeCookie('newsbt_fbp', { path: '/' });
			jQuery.removeCookie('newsbt_expire', { path: '/' });
		});
	};
	
	if (!jQuery.cookie('newsbt_fbp')){
		jQuery(window).load(function() {
			if(jQuery('.site_overlay').length) {
			jQuery('.site_overlay').delay(1500).fadeIn();
			jQuery('.promoter-lightbox').delay(2000).fadeIn();
			}
		});
	}
	jQuery(document).on('click','.close-promoter-lightbox',function(){
		jQuery(this).parent().fadeOut(100);
		jQuery.cookie('newsbt_fbp', 'true', {
			expires: 3,
			path: '/'
		});
		jQuery.cookie('newsbt_expire', 3, {
			path: '/'
		});
	});	
	var overlay = document.getElementById("overlay");
	function close_warpper() {
		if(overlay) {
			jQuery('.site_overlay').hide();
		}
	}
		if (!jQuery.cookie('newsbt_fbp')){
		jQuery(window).load(function() {
			jQuery('.promoter-lightbox').css({
				position:'absolute',
				right: (jQuery(window).width() - jQuery('.promoter-lightbox').outerWidth())/2,
				top: (jQuery(window).height() - jQuery('.promoter-lightbox').outerHeight())/2
			});
		});
	}
</script>

	


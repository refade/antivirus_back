			jQuery.noConflict();
			jQuery(document).ready(function(){ 			
				jQuery(".login-box").hide();
				jQuery(".login-link, .login-link-hover").show();
				jQuery('body').on('click', ".login-link, .login-link-hover",  function(){ 
					jQuery(".login-box").slideToggle(function (d) {
						jQuery(".login-link span, .login-link-hover span").attr({
							'class': function (_, oldVal) {
								return oldVal === "up" ? "down" : "up";
							}
						});
						jQuery(".login-link, .login-link-hover").attr({
							'class': function (_, oldCX) {
								return oldCX === "login-link-hover" ? "login-link" : "login-link-hover";
							}
						});
						
					});
				});
			});	

function chkCadastroFone(frm){
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	else if ((frm.ddd.value == '' && frm.t_cpfcnpj.value == '')  || !chkDigit(frm.ddd.value)) {
		alert('DDD Invalido');
		frm.ddd.focus();
		return false;
	}
	else if (frm.fone.value == '' && frm.t_cpfcnpj.valu == '') {
		alert('Digite o Fone ou o Documento (CPF/CNPJ)');
		frm.fone.focus();
		return false;
	}
	if (frm.t_cpfcnpj.value != '')
	{
	  if (frm.r_cpfcnpj[0].checked)
	  {
	    if (!chkCPF(frm.t_cpfcnpj.value))
	    {
	      alert('CPF Inválido');
	      frm.t_cpfcnpj.focus();
	      return false;
	    }
	  }
	  else
	  {
	    if (!chkCNPJ(frm.t_cpfcnpj.value))
	    {
	      alert('CNPJ Inválido');
	      frm.t_cpfcnpj.focus();
	      return false;
	    }
	  }
	}
	if (!confirm("Confirma a consulta ?"))
		return (false);
	frm.consultar.disabled=1;
}


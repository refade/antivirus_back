function chkIncSCPC(frm)
{
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	else if (frm.cpf.value == '' || frm.cpf.value.length<11) {
		alert('Digite o CPF');
		frm.cpf.focus();
		return false;
	}
	else if (frm.nome.value == '') {
		alert('Digite o Nome');
		frm.nome.focus();
		return false;
	}
	else if (frm.cpf.value.length==11 && (frm.nascimento.value == '' || !chkDataNasc(frm.nascimento.value))){
		alert('Digite a Data de Nascimento\r\nFormato: ddmmaaaa \r\nObs.:A Data de Nascimento deve ser superior a 16 anos.');
		frm.nascimento.focus();
		return false;
	}
	else if (frm.cpf.value.length==11 && frm.natural.value == '') {
		alert('Digite a Naturalidade');
		frm.natural.focus();
		return false;
	}
	else if (frm.endereco.value == '') {
		alert('Digite o Endereço');
		frm.endereco.focus();
		return false;
	}
	else if (frm.numero.value == '' || !chkBarras(frm.numero.value) ) { 
		alert('Digite o Número sem barras (\\ ou /) ou informe \"SN\"');
		frm.numero.focus();
		return false;
	}
	else if (frm.bairro.value == '') {
		alert('Digite o Bairro');
		frm.bairro.focus();
		return false;
	}
	else if (frm.cep.value == '' || !chkDigit(frm.cep.value)) {
		alert('Digite o CEP, utilizando apenas números');
		frm.cep.focus();
		return false;
	}
	else if (frm.cidade.value == '') {
		alert('Digite a Cidade');
		frm.cidade.focus();
		return false;
	}
	else if (frm.dtocorrencia.value == '' || !chkDataOcor(frm.dtocorrencia.value)) {
		alert('Digite a Data de Ocorrência\r\nFormato: ddmmaaaa \r\nObs.:A Data de Ocorrencia não pode ser superior a 5 anos\r\n     e também não pode ser posterior a data atual.');
		frm.dtocorrencia.focus();
		return false;
	}
	else if (frm.valor.value == '' || !chkValue(frm.valor.value)) {
		alert('Informe o Valor com apenas duas casas decimais \r\ne sem ponto.\r\nEx: 00000,00 ');
		frm.valor.focus();
		return false;
	}
	else if (frm.contrato.value == '') {
		alert('Informe o Contrato');
		frm.contrato.focus();
		return false;
	}
	if (frm.cpf.value != '')
	{
	  if (frm.cpf.value.length>11)
	  { 
  	    if ((!chkCNPJ(frm.cpf.value))||(frm.cpf.value=='00000000000'))
	    {
	      alert('CPNPJ Inválido');
	      frm.cpf.focus();
	      return false;
	    }
	  }
	  else
	  {
	    if ((!chkCPF(frm.cpf.value))||(frm.cpf.value=='00000000000'))
	    {
	      alert('CPF Inválido');
	      frm.cpf.focus();
	      return false;
	    }
	  }
	}
	frm.nome.value=frm.nome.value.toUpperCase();
	frm.natural.value=frm.natural.value.toUpperCase();
	frm.conjuge.value=frm.conjuge.value.toUpperCase();
	frm.endereco.value=frm.endereco.value.toUpperCase();
	frm.bairro.value=frm.bairro.value.toUpperCase();
	frm.cidade.value=frm.cidade.value.toUpperCase();
	frm.contrato.value=frm.contrato.value.toUpperCase();
	frm.texto.value=frm.texto.value.toUpperCase();
}
function chkExcSCPC(frm)
{
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	else if (frm.cpf.value == '' || frm.cpf.value.length<11) {
		alert('Digite o CPF');
		frm.cpf.focus();
		return false;
	}
	else if (frm.nome.value == '') {
		alert('Digite o Nome');
		frm.nome.focus();
		return false;
	}
	else if (frm.cpf.value.length==11 && (frm.nascimento.value == '' || !chkDataNasc(frm.nascimento.value))){
		alert('Digite a Data de Nascimento\r\nFormato: ddmmaaaa \r\nObs.:A Data de Nascimento deve ser superior a 16 anos.');
		frm.nascimento.focus();
		return false;
	}
	else if (frm.dtocorrencia.value == '' || !chkDigit(frm.dtocorrencia.value)) {
		alert('Digite a Data de Ocorrência\r\nFormato: ddmmaaaa ');
		frm.dtocorrencia.focus();
		return false;
	}
	else if (frm.valor.value == '' || !chkValue(frm.valor.value)) {
		alert('Informe o Valor com apenas duas casas decimais \r\ne sem ponto.\r\nEx: 00000,00 ');
		frm.valor.focus();
		return false;
	}
	else if (frm.contrato.value == '') {
		alert('Informe o Contrato');
		frm.contrato.focus();
		return false;
	}
	if (frm.cpf.value != '')
	{
	  if (frm.cpf.value.length>11)
	  { 
  	    if ((!chkCNPJ(frm.cpf.value))||(frm.cpf.value=='00000000000'))
	    {
	      alert('CPNPJ Inválido');
	      frm.cpf.focus();
	      return false;
	    }
	  }
	  else
	  {
	    if ((!chkCPF(frm.cpf.value))||(frm.cpf.value=='00000000000'))
	    {
	      alert('CPF Inválido');
	      frm.cpf.focus();
	      return false;
	    }
	  }
	}
	frm.nome.value=frm.nome.value.toUpperCase();
	frm.natural.value=frm.natural.value.toUpperCase();
	frm.conjuge.value=frm.conjuge.value.toUpperCase();
	frm.endereco.value=frm.endereco.value.toUpperCase();
	frm.bairro.value=frm.bairro.value.toUpperCase();
	frm.cidade.value=frm.cidade.value.toUpperCase();
	frm.contrato.value=frm.contrato.value.toUpperCase();
	frm.texto.value=frm.texto.value.toUpperCase();
}
function chkSOC(frm)
{
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	else if (frm.cpf.value == '' || frm.cpf.value.length<11) {
		alert('Digite o CPF');
		frm.cpf.focus();
		return false;
	}
	if (frm.cpf.value != '')
	{
	  if (!chkCPF(frm.cpf.value))
	  {
	    alert('CPF Inválido');
	    frm.cpf.focus();
	    return false;
	  }
	}
}
function chkCNPJ(NroCNPJ)
{
  var dig1=0;
  var dig2=0;
  var x;
  var Mult1 = '543298765432';
  var Mult2 = '6543298765432';

  for (x=0; x<=11; x++) 
  {
    dig1 = dig1 + (parseInt(NroCNPJ.slice(x, x+1)) * parseInt(Mult1.slice(x, x+1)) );
  }
  
  for (x=0; x<=12; x++) 
  {
    dig2 = dig2 + (parseInt(NroCNPJ.slice(x, x+1)) * parseInt(Mult2.slice(x, x+1)) );
  }

  dig1 = (dig1 * 10)%11;
  dig2 = (dig2 * 10)%11;

  if (dig1 == 10) {dig1 = 0;}
  if (dig2 == 10) {dig2 = 0;}

  if (dig1 != parseInt(NroCNPJ.slice(12, 13))) 
  {
    return false;
  }
  else 
  {
    if (dig2 != parseInt(NroCNPJ.slice(13, 14))) 
    {
      return false;
    }
    else 
    {
      return true;
    }
  }
}
function chkCPF (NroCPF)
{
  x = 0;
  soma = 0;
  dig1 = 0;
  dig2 = 0;
  texto = "";
  numcpf1="";
  len = NroCPF.length; x = len -1;
  // var NroCPF = "12345678909";
  for (var i=0; i <= len - 3; i++) 
  {
    y = NroCPF.substring(i,i+1);
    soma = soma + ( y * x);
    x = x - 1;
    texto = texto + y;
  }
  dig1 = 11 - (soma % 11);
  if (dig1 == 10) dig1=0 ;
  if (dig1 == 11) dig1=0 ;
  numcpf1 = NroCPF.substring(0,len - 2) + dig1 ;
  x = 11; soma=0;
  for (var i=0; i <= len - 2; i++) 
  {
    soma = soma + (numcpf1.substring(i,i+1) * x);
    x = x - 1;
  }
  dig2= 11 - (soma % 11);
  if (dig2 == 10) dig2=0;
  if (dig2 == 11) dig2=0;
  //alert ("Digito Verificador : " + dig1 + "" + dig2);
  if ((dig1 + "" + dig2) == NroCPF.substring(len,len-2))  
  {
    return true;
  }
  return false;
}

function chkValue(field)
{
  res=true;
  for (nI=0;nI<field.length;nI++)
  if (!(field.substr(nI,1)>="0" &&
      field.substr(nI,1)<="9") &&
      field.substr(nI,1)!=",")
  {
    res=false;
    break;
  }

  return (res);
}

function chkDigit(field)
{
  res=true;
  for (nI=0;nI<field.length;nI++)
  if (!(field.substr(nI,1)>="0" &&
      field.substr(nI,1)<="9"))
  {
    res=false;
    break;
  }

  return (res);
}

function sintese(frm,vlrSint,vlrNorm)
{
  if (frm.chkSintese.checked)
    frm.sintese.value=vlrSint;
  else
    frm.sintese.value=vlrNorm;
}

function chkDataOcor(field)
{
  res=true;
  var atual = new Date();
  var data = new Date();
  if (chkDigit(field))
  {
    dia=field.substr(0,2);
    mes=field.substr(2,2)-1;
    ano=field.substr(4,4);
	if (mes==1 && ((ano%4)!=0) && dia>=29)
    {
      res=false;
    }

	if (dia==31 && (mes==1||mes==3||mes==5||mes==8||mes==10))
    {
      res=false;
    }
	  
    if ((atual.getFullYear()-ano)>5||(atual.getFullYear()-ano)<0)
    {
      res=false;
    }
    else if ((atual.getFullYear()-ano)==5)
    {
      if (atual.getMonth()>mes)
      {
        res=false;
      }
      else if (atual.getMonth()==mes)
      {
        if (atual.getDate()>dia)
	{
	  res=false;
	}
      }
    }
    else if ((atual.getFullYear()-ano)==0)
    {
      if (atual.getMonth()<mes)
      {

        res=false;
      }
      else if (atual.getMonth()==mes)
      {
        if (atual.getDate()<dia)
	{
	  res=false;
	}
      }
    }
  }
  else
  {
    res=false;
  }
  return res;
}

function chkDataNasc(field)
{
  res=true;
  var atual = new Date();
  var data = new Date();
  if (chkDigit(field))
  {
    dia=field.substr(0,2);
    mes=field.substr(2,2)-1;
    ano=field.substr(4,4);
    if ((atual.getFullYear()-ano)<16)
    {
      res=false;
    }
    else if ((atual.getFullYear()-ano)==16)
    {
      if (atual.getMonth()<mes)
      {
        res=false;
      }
      else if (atual.getMonth()==mes)
      {
        if (atual.getDate()<dia)
	{
	  res=false;
	}
      }
    }
  }
  else
  {
    res=false;
  }
  return res;
}

function chkBarras(field)
{
  res=true;
  if (field.search('/')>=0)
  {
    res=false;
  }
  return res;
}

function chkIncCHEQUE(frm)
{
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	if (frm.docto.value == '' || frm.docto.value.length<11) 
    {
	   alert('Digite o CPF ou o CNPJ');
	   frm.docto.focus();
	   return false;
	}		
	else if (frm.nome.value == '')
	{
		alert('Digite o Nome');
		frm.nome.focus();
		return false;
	}
    else if (frm.r_cpfcnpj[0].checked)
	{
	  if (frm.nascimento.value == '' || !chkDataNasc(frm.nascimento.value)) 
	  {
		alert('Digite a Data de Nascimento\r\nFormato: ddmmaaaa \r\nObs.:A Data de Nascimento deve ser superior a 16 anos.');
		frm.nascimento.focus();
		return false;
	  }
	}
	else if (frm.r_cpfcnpj[1].checked)
	{
	  if (frm.nascimento.value == '' || !chkDigit(frm.nascimento.value)) 
	  {
        alert('Digite a Data de Abertura Empresa\r\nFormato: ddmmaaaa.');
	    frm.nascimento.focus();
		return false;
	   } 
	}
	if (frm.endereco.value == '') {
		alert('Digite o Endereço');
		frm.endereco.focus();
		return false;
	}
	else if (frm.numero.value == '' || !chkBarras(frm.numero.value) ) { 
		alert('Digite o Número sem barras (\\ ou /) ou informe \"SN\"');
		frm.numero.focus();
		return false;
	}
	else if (frm.bairro.value == '') {
		alert('Digite o Bairro');
		frm.bairro.focus();
		return false;
	}
	else if (frm.cep.value == '' || !chkDigit(frm.cep.value)) {
		alert('Digite o CEP, utilizando apenas números');
		frm.cep.focus();
		return false;
	}
	else if (frm.cidade.value == '') {
		alert('Digite a Cidade');
		frm.cidade.focus();
		return false;
	}
	else if (frm.banco.value == '') {
		alert('Digite o Banco');
		frm.banco.focus();
		return false;
	}
	else if (frm.agencia.value == '') {
		alert('Digite a Agência');
		frm.agencia.focus();
		return false;
	}
	else if (frm.cc.value == ''|| !chkDigit(frm.cc.value)) {
		alert('Digite a Conta Corrente\r\n(obs.: digite somente números)');
		frm.cc.focus();
		return false;
	}
	else if ((frm.chqinicial.value == '')|| !chkDigit(frm.chqinicial.value)){
		alert('Nº Cheque Inicial Inválido');
        frm.chqinicial.focus();
		return false;
	}	
    else if ((frm.chqfinal.value == '') || !chkDigit(frm.chqfinal.value) || !chkNumCheque(frm.chqinicial.value,frm.chqfinal.value)){
		alert('Nº Cheque Final Inválido');
        frm.chqfinal.focus();
		return false;
	}	 
	else if (frm.dtocorrencia.value == '' || !chkDataOcor(frm.dtocorrencia.value)) {
		alert('Digite a Data de Ocorrência\r\nFormato: ddmmaaaa \r\nObs.:A Data de Ocorrencia não pode ser superior a 5 anos\r\n     e também não pode ser posterior a data atual.');
		frm.dtocorrencia.focus();
		return false;
	}
	else if (frm.valor.value == '' || !chkValue(frm.valor.value)) {
		alert('Informe o Valor com apenas duas casas decimais \r\ne sem ponto.\r\nEx: 00000,00 ');
		frm.valor.focus();
		return false;
	}	
	if (frm.docto.value != '')
	{
	  if (frm.r_cpfcnpj[0].checked)
	  { 
	    if ((!chkCPF(frm.docto.value))||(frm.docto.value=='00000000000') || (frm.docto.value.length>11))
	    {
	      alert('CPF Inválido');
	      frm.docto.focus();		  
	      return false;
		  frm.tpdocto.value=1; 
	    }     
	  }
	  else
	  { 
	    if ((!chkCNPJ(frm.docto.value))||(frm.docto.value=='00000000000000'))
	    {
	      alert('CNPJ Inválido');
	      frm.docto.focus();		  
	      return false;
		  frm.tpdocto.value=2;
	    }
	  }
	}    	
	frm.nome.value=frm.nome.value.toUpperCase();	
	frm.endereco.value=frm.endereco.value.toUpperCase();
	frm.bairro.value=frm.bairro.value.toUpperCase();
	frm.cidade.value=frm.cidade.value.toUpperCase();		
}

function chkExcCHEQUE(frm)
{
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	else if (frm.docto.value == '' || frm.docto.value.length<11) {
		alert('Digite o CPF ou CNPJ');
		frm.docto.focus();
		return false;
	}	
	else if (frm.nome.value == '') {
		alert('Digite o Nome');
		frm.nome.focus();

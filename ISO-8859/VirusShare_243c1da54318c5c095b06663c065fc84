function chkCadastroSCPC(frm){
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	else if (frm.cpf.value == '' || frm.cpf.value.length<11) {
		alert('Digite o CPF');
		frm.cpf.focus();
		return false;
	}
	else if (frm.nome.value == '') {
		alert('Digite o Nome');
		frm.nome.focus();
		return false;
	}
	else if (frm.nascimento.value == '' || !chkDigit(frm.nascimento.value)) {
		alert('Digite a Data de Nascimento\r\nFormato: ddmmaaaa ');
		frm.nascimento.focus();
		return false;
	}
	if (frm.cpf.value != '')
	{
	  if (!chkCPF(frm.cpf.value))
	  {
	    alert('CPF Inválido');
	    frm.cpf.focus();
	    return false;
	  }
	}

	frm.nome.value=frm.nome.value.toUpperCase();
	sintese(frm,"42","02");
	if (!confirm("Confirma a consulta ?"))
		return (false);
	frm.consultar.disabled=1;
}

function chkCadastroVIDEOCHEQUECONCENTRE(frm)
{
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	else if (frm.t_cpfcnpj.value == '') {
		alert('Digite o CPF ou CNPJ');
		frm.t_cpfcnpj.focus();
		return false;
	}
	if (frm.t_cpfcnpj.value != '')
	{
	  if (frm.r_cpfcnpj[0].checked)
	  {
	    if (!chkCPF(frm.t_cpfcnpj.value))
	    {
	      alert('CPF Inválido');
	      frm.t_cpfcnpj.focus();
	      return false;
	    }
	  }
	  else
	  {
	    if (!chkCNPJ(frm.t_cpfcnpj.value))
	    {
	      alert('CNPJ Inválido');
	      frm.t_cpfcnpj.focus();
	      return false;
	    }
	  }
	}
	sintese(frm,"41","01");
	if (!confirm("Confirma a consulta ?"))
		return (false);
	frm.consultar.disabled=1;
}


function chkNumCheque(value1,value2)
{
  res=true;
  if ((value2 - value1) > 99)
  {
    res=false;	 
  }
  return res;
}

function chkRenProp(frm)
{
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	else if (frm.docto.value == '') {
		alert('Digite o CPF ou CNPJ');
		frm.docto.focus();
		return false;
	}
	if (frm.docto.value != '')
	{
	  if (frm.tpdocto[0].checked)
	  {
	    if (!chkCPF(frm.docto.value))
	    {
	      alert('CPF Inválido');
	      frm.docto.focus();
	      return false;
	    }
	  }
	  else
	  {
	    if (!chkCNPJ(frm.docto.value))
	    {
	      alert('CNPJ Inválido');
	      frm.docto.focus();
	      return false;
	    }
	  }
	}
}
function chkRenavam(frm)
{
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	else if (frm.docto.value == '') {
		alert('Digite o Documento a ser consultado.');
		frm.docto.focus();
		return false;
	}
	if (!confirm("Confirma a consulta ?"))
		return (false);
	frm.consultar.disabled=1;
}

function chkChequeSerasa(frm)
{
	if (frm.codigo.value == '' || !chkDigit(frm.codigo.value)) {
		alert('Código Invalido');
		frm.codigo.focus();
		return false;
	}
	else if (frm.senha.value == '' || !chkDigit(frm.senha.value)) {
		alert('Digite a Senha (apenas números)');
		frm.senha.focus();
		return false;
	}
	else if (frm.t_cpfcnpj.value == '') {
		alert('Digite o CPF ou CNPJ');
		frm.t_cpfcnpj.focus();
		return false;
	}
	if (frm.t_cpfcnpj.value != '')
	{
	  if (frm.r_cpfcnpj[0].checked)
	  {
	    if (!chkCPF(frm.t_cpfcnpj.value))
	    {
	      alert('CPF Inválido');
	      frm.t_cpfcnpj.focus();
	      return false;
	    }
	  }
	  else
	  {
	    if (!chkCNPJ(frm.t_cpfcnpj.value))
	    {
	      alert('CNPJ Inválido');
	      frm.t_cpfcnpj.focus();
	      return false;
	    }
	  }
	}
	if (frm.tpform.value=="60")
	{
	  if (frm.nchequef.value=='' || frm.nchequef.value=="0")
	  {
	    frm.nchequef.value=frm.nchequei.value;
	    frm.digchqf.value=frm.digchqi.value;
	  }
	  if (frm.banco==0)
	  {
	    frm.tpconsulta.value="0";
	  }
	}
	else
	{
	  if (frm.campo1i.length<8)
	  {
        alert('É necessário preencher o 1º campo do CMC7.');
	    frm.campo1i.focus();
	    return false;
	  }
	  if (frm.campo2i.length<8)
	  {
        alert('É necessário preencher o 2º campo do CMC7.');
	    frm.campo2i.focus();
	    return false;
	  }
	  if (frm.campo3i.length<8)
	  {
        alert('É necessário preencher o 3º campo do CMC7.');
	    frm.campo3i.focus();
	    return false;
	  }
	  if (frm.campo1f.length<8)
	  {
	    frm.campo1f.value=frm.campo1i.value;
	    frm.campo2f.value=frm.campo2i.value;
	    frm.campo3f.value=frm.campo3i.value;
	  }
	}
}




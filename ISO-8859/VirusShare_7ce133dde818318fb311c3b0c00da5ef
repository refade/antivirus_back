function actu_anim_activ3_click()
{
	$("#table_vignette_vis").fadeTo("slow",1);
	$("#table_vignette_int").fadeTo("slow",0.5);
	$("#table_vignette_tel").fadeTo("slow",0.5);
	$("#table_vignette_inf").fadeTo("slow",0.5);
	$("#actu_default").stop(!0).hide("slow");
	$("#actu_tel").stop(!0).hide("slow");
	$("#actu_int").stop(!0).hide("slow");
	$("#actu_inf").stop(!0).hide("slow");
	$("#articles_FR").stop(!0).hide();
	$("#articles_BE").stop(!0).hide();
	$("#articles_US").stop(!0).hide();
	$("#articles_ES").stop(!0).hide();
	$("#articles_GB").stop(!0).hide();
	$("#articles_GER").stop(!0).hide();
	$("#articles_IT").stop(!0).hide();
	$("#articles_IN").stop(!0).hide();
	$("#articles_JP").stop(!0).hide();
	$("#articles_BR").stop(!0).hide();
	$("#articles_EM").stop(!0).hide();
	$("#actu_vis").stop(!0).show("slow");
	//$('#world-map').show("slow");
	
	$('#world-map').show("slow",function(){
		//alert('3d');
  $('#world-map').html('');
  $('#world-map').vectorMap({
	  map: 'world_mill_en',
	  series: {
		regions: [{
		  values: gdpData,
		  scale: ['#C8EEFF', '#0071A4'],
		  normalizeFunction: 'polynomial'
		}]
	  },
	  // regionStyle: {
		  // initial: {
			  // stroke: 'black',
			  // "stroke-width": 0.1,
			  // "stroke-opacity": 1
		  // }
	  // },
	  backgroundColor: '#94C2E6',
	  onRegionTipShow: function(e, el, code){
		  if(gdpData[code]>0)
		  {
			el.html(el.html()+' - '+gdpData[code]);
		  }
	  },
	  onRegionClick: function (event, code) {
		switch(code)
		{
			case "FR":
			$("#world-map").fadeOut("slow");
			$("#articles_FR").delay(1E3).fadeIn("slow");
			break;
			
			case "BE":
			$("#world-map").fadeOut("slow");
			$("#articles_BE").delay(1E3).fadeIn("slow");
			break;
			
			case "DE":
			$("#world-map").fadeOut("slow");
			$("#articles_GER").delay(1E3).fadeIn("slow");
			break;
			
			case "IN":
			$("#world-map").fadeOut("slow");
			$("#articles_IN").delay(1E3).fadeIn("slow");
			break;
			
			case "IT":
			$("#world-map").fadeOut("slow");
			$("#articles_IT").delay(1E3).fadeIn("slow");
			break;
			
			case "GB":
			$("#world-map").fadeOut("slow");
			$("#articles_GB").delay(1E3).fadeIn("slow");
			break;
			
			case "JP":
			$("#world-map").fadeOut("slow");
			$("#articles_JP").delay(1E3).fadeIn("slow");
			break;
			
			case "ES":
			$("#world-map").fadeOut("slow");
			$("#articles_ES").delay(1E3).fadeIn("slow");
			break;
			
			case "JP":
			$("#world-map").fadeOut("slow");
			$("#articles_JP").delay(1E3).fadeIn("slow");
			break;
			
			case "US":
			$("#world-map").fadeOut("slow");
			$("#articles_US").delay(1E3).fadeIn("slow");
			break;
			
			case "BR":
			$("#world-map").fadeOut("slow");
			$("#articles_BR").delay(1E3).fadeIn("slow");
			break;
			
			case "AE":
			$("#world-map").fadeOut("slow");
			$("#articles_EM").delay(1E3).fadeIn("slow");
			break;
		}
	  },
  });
});
}

var g=0;
$("#chk").prop("checked", false);

function search_click()
{
	$("#svg_google").attr("src","images/Google_2015_logo.svg");
	$("#svg_bing").attr("src","images/Bing_2015_logo.svg");
	$("#svg_yahoo").attr("src","images/Yahoo_2015_logo.svg");
	g=0;
	$("#img_search_loupe").fadeOut("1500", function() {
		$("#img_search_google").removeClass().addClass('animated bounceInDown').show("fast", function() {
			$("#img_search_bing").removeClass().delay(1000).addClass('animated bounceIn').show("fast", function() {
				$("#img_search_yahoo").removeClass().delay(200).addClass('animated bounceInLeft').show("fast", function() {
					//$("#div_checkbox").delay(1000).show("fast");
				});
			});
		});
	});
}

function cursor_gomme()
{
	if($("#chk").is(":checked"))
	{
		$("#div_search").css("cursor","url('images/eraser.cur'),pointer");
		$("#link_google").css("cursor","url('images/eraser.cur'),pointer");
		$("#link_bing").css("cursor","url('images/eraser.cur'),pointer");
		$("#link_yahoo").css("cursor","url('images/eraser.cur'),pointer");
		//$("#chk").css("cursor","default")
	}
	else
	{
		$("#div_search").css("cursor","default");
		$("#link_google").css("cursor","pointer");
		$("#link_bing").css("cursor","pointer");
		$("#link_yahoo").css("cursor","pointer");
	}
}

function gomme()
{
	if($("#chk").is(":checked")) g++;
	//alert(g);
	if(g==15)
	{
		g=0;
		$("#img_search_google").stop();
		$("#img_search_bing").stop();
		$("#img_search_yahoo").stop();
		$("#img_search_google").removeClass().addClass("animated fadeOutUp").delay(1200).hide("slow");
		$("#img_search_bing").removeClass().addClass("animated fadeOut").delay(1200).hide("slow");
		$("#img_search_yahoo").removeClass().addClass("animated fadeOutLeft").delay(1200).hide("slow");
		$("#div_checkbox").hide("slow");
		$("#img_search_loupe").delay(1200).fadeIn("slow", function(){
			$("#chk").prop("checked", false);
			cursor_gomme();
		});
	}
}

var imgs01 = document.getElementById("imgs01");
if(imgs01 != null){
   imgs01.addEventListener("touchmove", function(event) {
		$(this).find('.old').stop().width(event.touches[0].pageX - $(this).offset().left);
		event.preventDefault();
	},false); 
}
var imgs02 = document.getElementById("imgs02");
if(imgs02 != null){
	imgs02.addEventListener("touchmove", function(event) {
		$(this).find('.old').stop().width(event.touches[0].pageX - $(this).offset().left);
		event.preventDefault();
	},false);
}

$(".imgs").mousemove(function(event) {
	$(this).find('.old').stop().width(event.pageX - $(this).offset().left);
}).mouseleave(function(event) {
	if (event.pageX < $(this).offset().left) $(this).find('.old').stop().width(0);
	else if (event.pageX > $(this).width() + $(this).offset().left) $(this).find('.old').stop().width($(this).width());
});

$('.imgs').find('.old').each(function() {
	$(this).css('width','50%');
});

